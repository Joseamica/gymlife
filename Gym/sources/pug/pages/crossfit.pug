							tag: { class: 'post-tag-secondary', text: 'Health' },
							time: { date: '2019-04-15', text: 'April 15, 2019' }
						})
				.row.row-30
					.col-sm-6
						+post-creative({
							img: 'post-19-448x299',
							title: 'What It Really Takes to Get Six Pack Abs',
							tag: { class: 'post-tag-primary', text: 'Tips' },
							time: { date: '2019-05-22', text: 'May 22, 2019' }
						})
					.col-sm-6
						+post-creative({
							img: 'post-20-448x299',
							title: 'Give your fitness a boost with our Crossfit challenge',
							tag: { class: 'post-tag-primary', text: 'Crossfit' },
							time: { date: '2019-01-18', text: 'February 18, 2019' }
						})
				.row.row-30
					.col-sm-6
						+post-creative({
							img: 'post-21-448x299',
							title: 'How to Tackle Your Favorite CrossFit Benchmark Workout',
							tag: { class: 'post-tag-yellow', text: 'Workouts' },
							time: { date: '2019-04-15', text: 'April 15, 2019' }
						})
					.col-sm-6
						+post-creative({
							img: 'post-22-448x299',
							title: '6 Practical Tips for Prioritizing Quality in CrossFit',
							tag: { class: 'post-tag-primary', text: 'Tips' },
							time: { date: '2019-03-12', text: 'March 12, 2019' }
						})

	//CTA
	section.section.section-lg.bg-default.text-center
		.container.wow.fadeInUp
			h6.title-8 Join our studio
			h2.font-weight-bold It's time to join us
			p.big Get in touch to book yourself a place for our classes.
			a.button.button-lg.button-primary( href='#' ) Get a free workout

	//Gallery
	section.section
		.owl-carousel.owl-style-3( data-items='1', data-sm-items='2', data-md-items='3', data-lg-items='4', data-mouse-drag='false', data-dots='true', data-loop='false', data-lightgallery='group' )
			+thumbnail-modern({
				img: 'gallery-16-480x362',
				imgOriginal: 'gallery-16-1200x800-original'
			}).wow.fadeIn
			+thumbnail-modern({
				img: 'gallery-17-480x362',
				imgOriginal: 'gallery-17-1200x800-original'
			}).wow.fadeIn( data-wow-delay='.05s' )
			+thumbnail-modern({
				img: 'gallery-18-480x362',
				imgOriginal: 'gallery-18-1200x800-original'
			}).wow.fadeIn( data-wow-delay='.1s' )
			+thumbnail-modern({
				img: 'gallery-19-480x362',
				imgOriginal: 'gallery-19-1200x800-original'
			}).wow.fadeIn( data-wow-delay='.15s' )

	//Google Map
	section.section
		.section-xl.bg-gray-100.google-map-style-1
			.container.contacts-paul-wrap: .row.justify-content-sm-end: .col-sm-6.col-md-5.wow.fadeInRight
				ul.contacts-paul
					li: .unit.align-items-center
						.unit-left: span.icon.mdi.mdi-map-marker
						.unit-body: a.big( href='#' ) 652 Main Road, Apt 12
							br
							| New York, USA 10033
					li: .unit.align-items-center
						.unit-left: span.icon.mdi.mdi-cellphone-android
						.unit-body: a.heading-5( href='tel:#' ) +1 800 123 1234
					li: .unit.align-items-center
						.unit-left: span.icon.mdi.mdi-email-outline
						.unit-body: a.big( href='mailto:#' ) info@demolink.org
			include ../includes/_google-map

	include ../sections/_footer-modern