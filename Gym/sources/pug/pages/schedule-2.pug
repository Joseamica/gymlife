			//Filters list
			ul.filters-list
				li: button.filter.active( data-filter='*' ) All Classes
				li: button.filter( data-filter='category-0' ) Crossfit
				li: button.filter( data-filter='category-1' ) Yoga
				li: button.filter( data-filter='category-2' ) Endurance
				li: button.filter( data-filter='category-3' ) Pilates

