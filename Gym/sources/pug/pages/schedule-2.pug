						li: span 9:00 AM
						li: span 10:00 AM
						li: span 11:00 AM
						li: span 12:00 PM
						li: span 1:00 PM
						li: span 2:00 PM
						li: span 3:00 PM
						li: span 4:00 PM
						li: span 5:00 PM
						li: span 6:00 PM
						li: span 7:00 PM

				.events
					ul
						li.events-group
							.events-day: span Monday
							ul
								+event({
									time: {
										start: '09:00', end: '11:00'
									},
									title: 'Crossfit',
									name: 'Frank Johnson'
								})( data-category='category-0' )
